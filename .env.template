# Discord Bot Token
# Discord Developer Portalから取得したトークンを設定
DISCORD_BOT_TOKEN=your_bot_token_here

# Optional: ログレベル
# DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Optional: カスタム設定
# DEFAULT_NORMAL_TIMEOUT=1800  # 30分（秒）
# DEFAULT_WORK_TIMEOUT=7200    # 120分（秒）
# DEFAULT_GRACE_PERIOD=900     # 15分（秒）